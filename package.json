    "karma": "~5.0.9",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.2.0",
    "karma-jasmine-html-reporter": "^1.4.0",
    "prettier": "^2.0.5",
    "ts-node": "~8.10.1",
    "tslint": "~5.15.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "typescript": "~3.8.3"
