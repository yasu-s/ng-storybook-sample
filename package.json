{
  "name": "ng-storybook",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "compodoc": "compodoc -p tsconfig.base.json -d ./docs -s",
    "compodoc:json": "compodoc -p tsconfig.base.json -d ./.storybook -e json",
    "storybook:serve": "start-storybook -p 8080",
    "storybook": "yarn compodoc:json && yarn storybook:serve"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.0.9",
    "@angular/cdk": "^10.1.3",
    "@angular/common": "~10.0.9",
